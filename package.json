{
  "name": "Questioner-react",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --open --mode development",
    "build": "webpack --mode production",
    "test": "jest --update-snapshot",
    "coverage": "jest --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(gif|ttf|eot|svg|png|jpg|jpeg)$": "<rootDir>/src/setupTests.js",
      "moduleFileExtensions": [
        "jsx",
        "js",
        "json",
        "ts",
        "tsx",
        "node"
      ]
    },
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/components/common/Logout.jsx",
      "src/components/common/protectedRoute.jsx",
      "src/components/services",
      "src/helpers/exceptionHandler.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akinmyde/Questioner-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Akinmyde/Questioner-react/issues"
  },
  "homepage": "https://github.com/Akinmyde/Questioner-react#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "jest-transform-css": "^2.0.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "crypto-js": "^3.1.9-1",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "install": "^0.12.2",
    "jest-css-modules": "^2.0.0",
    "joi-browser": "^13.4.0",
    "jwt-decode": "^2.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "npm": "^6.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-toastify": "^4.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "url-loader": "^1.1.2"
  }
}
